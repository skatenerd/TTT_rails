<%=
"First player was: "+@first_player.to_s
%><%=
"<br/>".html_safe
%><%=
"Board after turn "+@turn.to_s+" was:"
%><%=
render :partial => "board", 
       :object => @board, 
       :locals=>{:game_id =>@game_id,:should_build_link=>@should_build_link}
%><%=
if @can_step_backward
  build_previous_link(@game_id,@turn)
end
%><%=
" "
%><%=
if @can_step_forward
  build_next_link(@game_id,@turn)
end
%>

