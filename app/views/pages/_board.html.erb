<%=
"<table>".html_safe%>
  <%
  row_index=0
  board.each do |row|
    %><%=
    "<tr>".html_safe
    %><%
    col_index=0
    row.each do |text|
      coordinates=[row_index,col_index]
      %><%=
      content_tag(:td,build_td(text,coordinates,game_id,winner))
      %><%
      col_index+=1
    end
    %><%=
    "</tr>".html_safe
    %><%
    row_index+=1
  end
  %><%=
"</table>".html_safe
%>
      
      

