<%= "<table>".html_safe%>
<%
  row_index=0
  board.each do |row|
    %><%= "<tr>".html_safe%><%
    col_index=0
    row.each do |text|
      coordinates=[row_index,col_index]
      updated_board=future_boards[coordinates]
      %><%=content_tag(:td,buildGameLink(text,updated_board,player),:name=>coordinates.to_s)%>
    <%
      col_index+=1
    end%>
    <%="</tr>".html_safe%><%
    row_index+=1
  end%>
<%="</table>".html_safe%>
      
      

